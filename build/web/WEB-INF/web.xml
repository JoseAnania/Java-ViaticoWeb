<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ServletAltaViatico</servlet-name>
        <servlet-class>Servlets.ServletAltaViatico</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletListaViatico</servlet-name>
        <servlet-class>Servlets.ServletListaViatico</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletTotal</servlet-name>
        <servlet-class>Servlets.ServletTotal</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletAltaViatico</servlet-name>
        <url-pattern>/ServletAltaViatico</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletListaViatico</servlet-name>
        <url-pattern>/ServletListaViatico</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletTotal</servlet-name>
        <url-pattern>/ServletTotal</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
